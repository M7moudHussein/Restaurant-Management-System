<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox id="myvbox" alignment="TOP_CENTER" prefHeight="488.0" prefWidth="585.0" stylesheets="@../css/style.css"
      xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ViewController">
    <HBox id="bossHbox" prefHeight="85.0" prefWidth="441.0">
        <ImageView id="bossImage " fitHeight="118.0" fitWidth="362.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets left="210.0"/>
            </HBox.margin>
            <Image url="@../photos/boss.jpg"/>
        </ImageView>
    </HBox>
    <HBox id="myhbox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="342.0" prefWidth="678.0" stylesheets="@../css/style.css">
        <Button id="menuBtn" mnemonicParsing="false" onMouseClicked="#showMenu" prefHeight="196.0" prefWidth="199.0"
                stylesheets="@../css/style.css" text="Button"/>
        <Button id="empBtn" mnemonicParsing="false" onMouseClicked="#showEmployees" prefHeight="196.0"
                prefWidth="199.0" stylesheets="@../css/style.css"/>
        <Button id="analysisBtn" mnemonicParsing="false" onMouseClicked="#showAnalysis" prefHeight="196.0"
                prefWidth="199.0" stylesheets="@../css/style.css"/>
    </HBox>
</VBox>
