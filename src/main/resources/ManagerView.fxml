<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox id="myvbox" alignment="TOP_CENTER" prefHeight="491.0" prefWidth="654.0" stylesheets="@style.css"
      xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ViewController">
    <children>
        <HBox id="bossHbox" prefHeight="85.0" prefWidth="441.0">
            <children>
                <ImageView id="bossImage " fitHeight="118.0" fitWidth="362.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@photos/boss.jpg"/>
                    </image>
                    <HBox.margin>
                        <Insets left="210.0"/>
                    </HBox.margin>
                </ImageView>
            </children>
        </HBox>
        <HBox id="myhbox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
              prefHeight="342.0" prefWidth="678.0" stylesheets="@style.css">
            <children>
                <Button id="menuBtn" onMouseClicked="#showMenu" mnemonicParsing="false" prefHeight="196.0"
                        prefWidth="199.0" stylesheets="@style.css" text="Button"/>
                <Button id="empBtn" onMouseClicked="#showEmployees" mnemonicParsing="false" prefHeight="196.0"
                        prefWidth="199.0" stylesheets="@style.css"/>
                <Button id="analysisBtn" onMouseClicked="#showAnalysis" mnemonicParsing="false" prefHeight="196.0"
                        prefWidth="199.0" stylesheets="@style.css"/>
            </children>
        </HBox>
    </children>
</VBox>
