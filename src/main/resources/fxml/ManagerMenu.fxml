<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>
<AnchorPane id="anchor" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.manager.MenuController">
    <stylesheets>
        <URL value="@../css/menu.css"/>
    </stylesheets>
    <ScrollPane id="scPane" fx:id="scPane" layoutX="10.0" layoutY="14.0" prefHeight="510.0" prefWidth="777.0"
                stylesheets="@../css/menu.css"/>
    <Button id="addBtn" alignment="CENTER" contentDisplay="CENTER" layoutX="539.0" layoutY="533.0"
            mnemonicParsing="false" onAction="#addDish" stylesheets="@../css/menu.css" text="Add Dish"
            textAlignment="RIGHT"/>
    <Button id="backBtn" layoutX="175.0" layoutY="533.0" mnemonicParsing="false" onAction="#goBack" prefHeight="45.0"
            prefWidth="120.0" stylesheets="@../css/menu.css" text="Back"/>
</AnchorPane>
